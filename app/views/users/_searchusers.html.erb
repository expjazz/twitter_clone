<div class="ui fluid category search">
  <%= form_with url: search_user_path, method: :get, local: true do %>
    <div class="ui icon input">
      <%= text_field_tag :user, params[:user], class: 'prompt'%>
      <i class="search icon"></i>
      

    </div>

  <% end %>
  <div class="result">
    <%= render 'users/searchresult'%>
  </div>
</div>